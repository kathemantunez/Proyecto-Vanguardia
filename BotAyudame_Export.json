{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "BotAyudame",
    "version": "1",
    "intents": [
      {
        "name": "Recordatorio",
        "version": "5",
        "fulfillmentActivity": {
          "type": "ReturnIntent"
        },
        "sampleUtterances": [
          "Agendar {record}",
          "Recordarme {record}",
          "Agregar tarjeta {MisTarjetasSlot}",
          "Mis tarjetas {MisTarjetasSlot}",
          "Tareas pendientes {Tareas}",
          "Agregar deposito {MisTarjetasSlot}",
          "Agregar retiro {MisTarjetasSlot}"
        ],
        "slots": [
          {
            "sampleUtterances": [],
            "slotType": "AMAZON.DATE",
            "obfuscationSetting": "NONE",
            "slotConstraint": "Required",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Qué día desea programarla?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 4,
            "name": "date"
          },
          {
            "sampleUtterances": [],
            "slotType": "MisTarjetasSlot",
            "slotTypeVersion": "1",
            "obfuscationSetting": "NONE",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Qué tipo de tarjeta desea agregar (VISA, MASTER CARD)?)"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 2,
            "name": "MisTarjetasSlot"
          },
          {
            "sampleUtterances": [],
            "slotType": "AMAZON.DATE",
            "obfuscationSetting": "NONE",
            "slotConstraint": "Required",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Está bien"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 6,
            "name": "record"
          },
          {
            "sampleUtterances": [],
            "slotType": "Tareas",
            "slotTypeVersion": "1",
            "obfuscationSetting": "NONE",
            "slotConstraint": "Required",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Qué tipo de tarea desea agregar (Salud, Oficio, Cita, Comprar)?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 3,
            "name": "Tareas"
          },
          {
            "sampleUtterances": [],
            "slotType": "AMAZON.TIME",
            "obfuscationSetting": "NONE",
            "slotConstraint": "Required",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "A qué hora?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 5,
            "name": "time"
          }
        ],
        "conclusionStatement": {
          "messages": [
            {
              "groupNumber": 1,
              "contentType": "PlainText",
              "content": "Guardado exitosamente!"
            }
          ]
        }
      }
    ],
    "slotTypes": [
      {
        "name": "MisTarjetasSlot",
        "version": "1",
        "enumerationValues": [
          {
            "value": "VISA"
          },
          {
            "value": "Master Card"
          }
        ],
        "valueSelectionStrategy": "ORIGINAL_VALUE"
      },
      {
        "name": "Tareas",
        "version": "1",
        "enumerationValues": [
          {
            "value": "Ejercicio"
          },
          {
            "value": "Salud"
          },
          {
            "value": "Cita"
          },
          {
            "value": "Comprar"
          },
          {
            "value": "Oficio"
          },
          {
            "value": "Trabajo"
          }
        ],
        "valueSelectionStrategy": "ORIGINAL_VALUE"
      }
    ],
    "voiceId": "Joanna",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 300,
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, can you please repeat that?"
        }
      ],
      "maxAttempts": 5
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I could not understand. Goodbye."
        }
      ]
    },
    "detectSentiment": true
  }
}